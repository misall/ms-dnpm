<!-- ======= Team Section ======= -->
<section id="team" class="team section-bg">
    <div class="container">

        <div class="section-title" >
            <h2>Activites</h2>
            <p>Nos recentes activites </p>
        </div>

        <div class="row" >

            {% for activite in activites %}
                <div class="col-4 mb-5">
                    <div class="member" >
                        <div class="pic"><img src="{{ activite.image }}" class="img-fluid" alt=""></div>
                        <div class="member-info">
                            <a href="#">
                                <h4 class="text-success">{{ activite.title }}</h4>
                                <span class="text-success">{{ activite.lieu }}</span>
                            </a>

                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>

    </div>
</section><!-- End Team Section -->